<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.jdt.core.classpathContainerInitializer">
      <classpathContainerInitializer
            class="github.javaappplatform.eclipse.project.classpath.AppClasspathContainerInitializer"
            id="github.javaappplatform.eclipse.project.classpath.AppClasspathContainer">
      </classpathContainerInitializer>
   </extension>
   <extension
         id="github.javaappplatform.eclipse.project.builder.JSONCheckBuilder"
         name="Java App Platform Builder"
         point="org.eclipse.core.resources.builders">
      <builder
            hasNature="true">
         <run
               class="github.javaappplatform.eclipse.project.builder.JSONCheckBuilder">
         </run>
      </builder>
   </extension>
   <extension
         id="github.javaappplatform.eclipse.project.builder.Nature"
         name="Java App Platform Nature"
         point="org.eclipse.core.resources.natures">
      <runtime>
         <run
               class="github.javaappplatform.eclipse.project.builder.Nature">
         </run>
      </runtime>
      <builder
            id="github.javaappplatform.eclipse.project.builder.JSONCheckBuilder">
      </builder>
      <requires-nature
            id="org.eclipse.jdt.core.javanature">
      </requires-nature>
   </extension>
</plugin>
